<!-- 归档页面，可选择性注释部分模块 -->

<% if (!is_year() && !is_month()) { %>
  <div class="archive">
    <h1 class="archive-count"><%= __('post_total_count', site.posts.length) %></h1>
  </div>

  <div class="archive list">
    <% if(site.posts.length) { %>
      <%- list_archives({format: 'YYYY MMMM', class: 'archive'}) %>
    <% } %>
  </div>
<% } %>

<%- partial('_partial/archive', {pagination: config.archive, index: true}) %>
