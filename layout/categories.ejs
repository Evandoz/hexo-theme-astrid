<!-- 分类页面，可选择性注释部分模块 -->

<div class="archive">
  <h1 class="archive-count"><%= __('category_total_count', site.categories.length) %></h1>
</div>

<div class="archive list">
  <% if(site.categories.length) { %>
    <%- list_categories({class: 'archive'}) %>
  <% } %>
</div>

<% site.categories.sort('name').map(function(category){  %>
  <%- partial('_partial/archive', {
    page: category, title: category.name, pagination: config.category, index: true
  }) %>
<% }) %>
